redir-port: 7892
allow-lan: true
dns:
  enable: true
  listen: 127.0.0.1:8053
  default-nameserver:
    - 223.5.5.5
    - 8.8.8.8
  enhanced-mode: redir-host # or fake-ip
  nameserver:
    - 223.5.5.5 # default value
    - 8.8.8.8 # default value
    - tls://dns.rubyfish.cn:853 # DNS over TLS
    - https://1.1.1.1/dns-query # DNS over HTTPS
allow-lan: false
mode: rule
